    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
</head>

<body>
    <!-- Parte superior de la pagina dividido en un box, con tres box internos -->
    <%- include("Partials/header") %>
    <!-- Cuerpo principal de la pagina dividido en 4 boxes, -->
    <main>
        <!--Galeria-->
       <!--< <div class="container-main0-all menu" >
            <i class="far fa-times-circle"></i>
        </div>-->
        <div class="container-main1-all">
            <div class="container-body">
                <div class="row0">
                    <div class="galeria">
                        <input type="radio" name="navegacion" id="_1" checked>
                        <input type="radio" name="navegacion" id="_2">
                        <img src="/img/image_novedades.jpeg" alt="Galeria CSS 1" />
                        <img src="/img/image_destacados.jpeg" alt="Galeria CSS 2" />
                    </div>
                </div>
            </div>
        </div>
        <!--Novedades y Destacados-->

        <div class="container-main2-all">
            <div class="container-body">
                <div class="row0">
                    <div class="colum0 ef_novedades_destacados">
                        <h1>DESTACADOS</h1>
                        <div><button class="boton_destacados">INGRESAR</button></div>
                    </div>
                    <div class="colum1 ef_novedades_destacados">
                        <h1>NOVEDADES</h1>
                        <div><button class="boton_novedades">INGRESAR</button></div>
                    </div>
                </div>
            </div>
        </div>
        <!--Articles-->
        <div class="container-main3-all">
            <div class="container-body">
                <div class="row0">
                    <div class="colum0 ef_producto">
                        <div class="producto">
                            <img src="/img/image_destacados.jpeg" alt="Producto 1" />
                            <a href="detalleProducto" target="_blank"><i class="fas fa-cart-plus"></i></a>
                        </div>
                        <div class="description">
                            <h3 class="nombre_del_producto"> NOMBRE PRODUCTO 1</h3>
                            <h4 class="precio_del_producto"> Precio</h4>
                            <h5 class="forma_de_pago"> Forma de pago / promociones</h5>
                        </div>
                    </div>
                    <div class="colum1 ef_producto">
                        <div class="producto">
                            <img src="/img/image_destacados.jpeg" alt="Producto 2" />
                            <a href="*"><i class="fas fa-cart-plus"></i></a>
                        </div>
                        <div class="description">
                            <h3 class="nombre_del_producto"> NOMBRE PRODUCTO 1</h3>
                            <h4 class="precio_del_producto"> Precio</h4>
                            <h5 class="forma_de_pago"> Forma de pago / promociones</h5>
                        </div>
                    </div>
                    <div class="colum2 ef_producto">
                        <div class="producto">
                            <img src="/img/image_destacados.jpeg" alt="Producto 3" />
                            <a href="*"><i class="fas fa-cart-plus"></i></a>
                        </div>
                        <div class="description">
                            <h3 class="nombre_del_producto"> NOMBRE PRODUCTO 1</h3>
                            <h4 class="precio_del_producto"> Precio</h4>
                            <h5 class="forma_de_pago"> Forma de pago / promociones</h5>
                        </div>
                    </div>
                    <div class="colum3 ef_producto">
                        <div class="producto">
                            <img src="/img/image_destacados.jpeg" alt="Producto 4" />
                            <a href="*"><i class="fas fa-cart-plus"></i></a>
                        </div>
                        <div class="description">
                            <h3 class="nombre_del_producto"> NOMBRE PRODUCTO 1</h3>
                            <h4 class="precio_del_producto"> Precio</h4>
                            <h5 class="forma_de_pago"> Forma de pago / promociones</h5>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Newsletter -->

        <section class="main_box_4">
            <div class="newsletter">
                <h3> Suscribite a nuestro newsletter </h3>
                <h4> ¡Registrate y disfrutá nuestras ofertas!</h4>
                <input class="register" type="text" placeholder="E-MAIL...">
                <button class="boton_enviar">ENVIAR</button>
            </div>
        </section>
        </div>
    </main>
    <!-- Parte inferior con dos cajas, una exclusiva para el copyright, y la otra subdividida en 3 boxes-->
    <%- include("Partials/footer") %>

</body>

</html>